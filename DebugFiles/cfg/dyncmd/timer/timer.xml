<?xml version="1.0" encoding="utf-8"?>
<DynamicCommand xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Enabled>true</Enabled>
  <Alias>timer</Alias>
  <Usage>timer &lt;time&gt;</Usage>
  <PermissionName>stats</PermissionName>
  
  <UserConfig>
    <OnNoParam>No time specified. {usage}</OnNoParam>
    <OnInvalidParam>Invalid input {input}. Expected numeric value (0~60).</OnInvalidParam>
    <OnSet>{admin} set timer to {value}.</OnSet>
  </UserConfig>
</DynamicCommand>